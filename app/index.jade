  body(ng-app="lime-topcoder", ng-controller="main as main")
    include layout/header.jade

    .view-container(ui-view, ng-class="$state.current.name")
    .view-container-loading(ng-show="main.loading")
      .circle-3-loading
        span.loading-wrapper
          span.outer-circle
          span.inner-circle
            span.circle

    include layout/footer.jade

    script(src="https://cdn.auth0.com/w2/auth0-1.6.4.js", type="text/javascript")
    script(src="//app-abc.marketo.com/js/forms2/js/forms2.min.js", type="text/javascript")

    // build:js({.,app}) scripts/vendor.js
    each bowerJsFile in bowerJsFiles
      script(src="bower_components/#{bowerJsFile}.js", type="text/javascript")
    each nonBowerJsFile in nonBowerJsFiles
      script(src="content/scripts/#{nonBowerJsFile}.js", type="text/javascript")
    // endbuild

    // build:js({.tmp,app}) scripts/app.js
    each jsFile in jsFiles
      script(src="#{jsFile}.js", type="text/javascript")
    // endbuild

